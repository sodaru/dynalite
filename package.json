{
  "name": "dynalite-sodaru",
  "version": "3.2.1",
  "description": "An implementation of Amazon's DynamoDB built on LevelDB",
  "main": "index.js",
  "bin": "cli.js",
  "scripts": {
    "build1": "pegjs ./db/conditionParser.pegjs",
    "build2": "pegjs ./db/projectionParser.pegjs",
    "build3": "pegjs ./db/updateParser.pegjs",
    "build": "npm run build1 && npm run build2 && npm run build3",
    "pretest": "npm run build",
    "test": "npx mocha --require should --reporter spec -t 30s",
    "coverage": "npx nyc@latest mocha --require should -t 4s",
    "prepack": "npm test",
    "postversion": "git push --follow-tags"
  },
  "repository": "mhart/dynalite",
  "keywords": [
    "dynamodb",
    "mock",
    "fake",
    "test",
    "aws",
    "dynamo",
    "leveldb"
  ],
  "author": "Michael Hart <michael.hart.au@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.3",
    "big.js": "^5.2.2",
    "buffer-crc32": "^0.2.13",
    "lazy": "^1.0.11",
    "levelup": "^4.4.0",
    "lock": "^1.1.0",
    "memdown": "^5.1.0",
    "minimist": "^1.2.5",
    "once": "^1.4.0",
    "subleveldown": "^5.0.0"
  },
  "optionalDependencies": {
    "leveldown": "^5.2.1"
  },
  "devDependencies": {
    "aws4": "^1.10.0",
    "mocha": "^6.2.3",
    "pegjs": "^0.10.0",
    "should": "^13.2.3"
  }
}
